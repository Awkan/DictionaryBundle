{
    "name":         "knplabs/dictionary-bundle",
    "description":  "Are you often tired to repeat static choices like gender or civility in your apps ?",
    "keywords":     ["dictionary"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name": "Knplabs",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "BiiG",
            "homepage": "https://www.biig.fr"
        },
        {
            "name": "Shivoham",
            "homepage": "https://github.com/Shivoham"
        },
        {
            "name": "PedroTroller",
            "homepage": "https://github.com/PedroTroller"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "symfony/http-kernel": "~2.8 || ~3.0 || ~4.0",
        "symfony/dependency-injection": "~2.8 || ~3.0 || ~4.0",
        "symfony/http-foundation": "~2.8 || ~3.0 || ~4.0",
        "symfony/config": "~2.8 || ~3.0 || ~4.0",
        "symfony/form": "~2.8 || ~3.0 || ~4.0",
        "symfony/validator": "~2.8 || ~3.0 || ~4.0",
        "twig/twig": "~1.12|~2.0",
        "fzaninotto/faker": "^1.5"
    },
    "require-dev": {
        "bossa/phpspec2-expect": "~2.0",
        "leanphp/phpspec-code-coverage": "~3.0",
        "phpspec/phpspec": "~3.0",
        "phpspec/prophecy": "~1.6"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Knp\\DictionaryBundle\\": "src/Knp/DictionaryBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    },
    "suggest": {
        "knplabs/rad-fixtures-load": "Allows to autoregister the dictionary provider as an alice provider"
    }
}

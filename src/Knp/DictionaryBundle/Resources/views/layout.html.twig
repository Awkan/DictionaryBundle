{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="28" height="28" alt="Dictionary" src="data:image/png;base64,">
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Dictionaries</b>
            <span>{{ collector.dictionaries.count }}</span>
        </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
<span class="icon">
    <img width="26" height="28" alt="Dictionary" src="data:image/png;base64,">
</span>
<strong>Dictionaries</strong>
<span class="count">
    <span>{{ collector.dictionaries.count }}</span>
</span>
{% endblock %}

{% block panel %}
<h2>Dictionaries</h2>
{% for dictionary in collector.dictionaries %}
    <h3>{{ dictionary.name }}</h3>
    <table>
        <tbody>
            <tr>
                <th>Key</th>
                <th>Value</th>
            </tr>
            {% for key, value in dictionary.values %}
            <tr>
                <td>{{ key }}</td>
                <td>{{ value }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
{% endfor %}
{% endblock %}
